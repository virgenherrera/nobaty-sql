{
  "name": "nobaty-sql",
  "version": "1.0.0",
  "description": "Take advantage of typescript with express js, sequelize, sequelize-cli, socket.io yarn and built-in tasks to quick creation of rest/webSocket service",
  "main": "./bin/www",
  "engines": {
    "node": ">=8.9.0",
    "npm": ">=5.5.1"
  },
  "keywords": [
    "node 8",
    "typescript 2.6"
  ],
  "contributors": [
    {
      "name": "Hugo Virgen",
      "email": "virgenherrera@gmail.com",
      "url": "https://github.com/virgenherrera/"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/virgenherrera/nobaty-sql"
  },
  "bugs": {
    "url": "https://github.com/virgenherrera/nobaty-sql/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/virgenherrera/nobaty-sql#readme",
  "scripts": {
    "start": "run-s -l cleanup lint ts:build serve:node",
    "run": "run-s -l cleanup lint ts:build serve:node",
    "run:dev": "run-s -l -c cleanup lint ts:build serve:development",
    "run:production": "run-s production:start",
    "ts:build": "./node_modules/.bin/tsc",
    "ts:build:watch": "./node_modules/.bin/tsc -w",
    "provision": "run-p -l -c provide:env provide-forever-config provide:db:config",
    "generate": "node ./tasks/createModule.js",
    "cleanup": "run-p clean:dist clean:logs",
    "sequelize": "./node_modules/.bin/sequelize",
    "nodemon": "./node_modules/.bin/nodemon",
    "forever": "./node_modules/.bin/forever",
    "lint": "node ./node_modules/tslint/bin/tslint -c tslint.json -p tsconfig.json",
    "production:start": "run-s -l cleanup lint ts:build provide-forever-config run:forever",
    "production:restart": "run-s -l run:forever:stop production:start",
    "production:stop": "run-s -l run:forever:stop cleanup",
    "serve:node": "node ./bin/www",
    "serve:development": "run-p ts:build:watch run:nodemon",
    "run:nodemon": "./node_modules/.bin/nodemon -w .env -w dist/ -V --inspect",
    "run:forever": "./node_modules/.bin/forever start forever.json",
    "run:forever:stop": "./node_modules/.bin/forever stop ./bin/www",
    "provide:db:config": "node ./tasks/provideDbConf.js",
    "provide:env": "node ./tasks/provideEnv.js",
    "provide-forever-config": "node ./tasks/provideForeverJson.js",
    "provide:persistence": "run-s -l ts:build provide:database provide:migrations provide:seeds",
    "provide:database": "./node_modules/.bin/sequelize db:create",
    "provide:migrations": "./node_modules/.bin/sequelize db:migrate",
    "provide:seeds": "./node_modules/.bin/sequelize db:seed:all",
    "clean:logs": "node ./tasks/cleanLogs.js",
    "clean:dist": "node ./tasks/cleanDist.js",
    "postinstall": "run-s provision"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "debug": "^3.1.0",
    "del": "^3.0.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "forever": "^0.15.3",
    "jsonwebtoken": "^8.1.0",
    "moment": "^2.19.1",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.1",
    "pg": "6",
    "pg-hstore": "^2.3.2",
    "pluralize": "^7.0.0",
    "pug": "^2.0.0-rc.4",
    "reflect-metadata": "^0.1.10",
    "sequelize": "^4.27.0",
    "sequelize-cli": "^3.2.0",
    "sequelize-typescript": "^0.6.1",
    "serve-favicon": "^2.4.5",
    "socket.io": "^2.0.4",
    "ts-node": "^3.3.0",
    "typescript": "^2.6.2",
    "uuid": "^3.1.0",
    "yargs": "^10.0.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.1",
    "@types/body-parser": "^1.16.7",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.1",
    "@types/debug": "^0.0.30",
    "@types/del": "^3.0.0",
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.0.39",
    "@types/jsonwebtoken": "^7.2.3",
    "@types/morgan": "^1.7.35",
    "@types/multer": "^1.3.6",
    "@types/node": "^8.0.47",
    "@types/pluralize": "^0.0.28",
    "@types/pug": "^2.0.4",
    "@types/sequelize": "^4.0.79",
    "@types/serve-favicon": "^2.2.29",
    "@types/socket.io": "^1.4.31",
    "tslint": "^5.8.0"
  }
}
